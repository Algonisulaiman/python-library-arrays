    "#  Write a Python program to convert Year/Month/Day to Day of Year in Python\n",
    "import datetime\n",
    "today = datetime.datetime.now()\n",
    "print(today)\n",
    "day_of_year = (today - datetime.datetime(today.year, 1, 1)).days + 1\n",
    "print(day_of_year)"
